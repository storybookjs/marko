{
  "name": "@storybook/marko-vite",
  "version": "2.0.1",
  "description": "Storybook for Marko and Vite: Develop Marko components in isolation with Hot Reloading.",
  "keywords": [
    "marko",
    "storybook",
    "framework",
    "vite"
  ],
  "homepage": "https://github.com/storybookjs/marko/blob/main/README.md",
  "bugs": "https://github.com/storybookjs/marko/issues/new?template=Bug_report.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/marko"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.mjs",
      "default": "./dist/preset.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "preset.js",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@marko/compiler": "^5",
    "@marko/vite": "^4",
    "@storybook/builder-vite": "^8.1.2",
    "@storybook/core-common": "^8.1.2",
    "@storybook/global": "^5.0.0",
    "@storybook/types": "^8.1.2"
  }
}
