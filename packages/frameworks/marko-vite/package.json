{
  "name": "@storybook/marko-vite",
  "version": "1.0.3",
  "description": "Storybook for Marko and Vite: Develop Marko components in isolation with Hot Reloading.",
  "keywords": [
    "marko",
    "storybook",
    "framework",
    "vite"
  ],
  "homepage": "https://github.com/storybookjs/marko/blob/main/README.md",
  "bugs": "https://github.com/storybookjs/marko/issues/new?template=Bug_report.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/marko"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./*.d.ts": "./*.d.ts",
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.mjs",
      "default": "./dist/preset.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "preset.js",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@babel/core": "^7",
    "@marko/compiler": "^5",
    "@marko/vite": "^4",
    "@storybook/builder-vite": "^7.5.3",
    "@storybook/core-common": "^7.5.3",
    "@storybook/global": "^5.0.0",
    "@storybook/types": "^7.5.3",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
