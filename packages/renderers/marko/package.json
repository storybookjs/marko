{
  "name": "@storybook/marko",
  "version": "10.0.0",
  "description": "Storybook for Marko: Develop Marko Components in isolation.",
  "keywords": [
    "marko",
    "renderer",
    "storybook"
  ],
  "homepage": "https://github.com/storybookjs/marko/blob/main/README.md",
  "bugs": "https://github.com/storybookjs/marko/issues/new?template=Bug_report.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/marko"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.mjs",
      "default": "./dist/preset.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "preview.js",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@storybook/global": "^5.0.0",
    "storybook": "^9.0.15"
  }
}
