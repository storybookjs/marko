{
  "name": "@storybook/marko",
  "private": true,
  "workspaces": [
    "packages/frameworks/*",
    "packages/renderers/*",
    "tests/frameworks/*"
  ],
  "scripts": {
    "build": "npm run build:types && node -r ~preload ./scripts/build.ts",
    "build:types": "tsc -b tsconfig.build.json",
    "change": "changeset add",
    "ci:test": "c8 npm test -- --forbid-only",
    "format": "eslint -f unix --fix . && prettier . --write --log-level=error && sort-package-json --quiet ./{,packages/*/*/,tests/frameworks/*/}package.json",
    "prepare": "husky install",
    "release": "npm run build && changeset publish",
    "test": "playwright test",
    "version": "changeset version && npm i --package-lock-only"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.22.11",
    "@babel/preset-typescript": "^7.22.11",
    "@babel/register": "^7.22.5",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@marko/compiler": "^5.33.1",
    "@playwright/test": "^1.37.1",
    "@testing-library/dom": "^9.3.1",
    "@types/babel__register": "^7.17.0",
    "@types/jsdom": "^21.1.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "@types/resolve": "^1.20.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "c8": "^8.0.1",
    "context-require": "^1.4.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "marko": "^5.31.6",
    "mocha": "^10.2.0",
    "mocha-snap": "^5.0.0",
    "prettier": "^3.0.3",
    "pretty-format": "^29.6.3",
    "resolve": "^1.22.4",
    "resolve.exports": "^2.0.2",
    "sort-package-json": "^2.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "~preload": "file:scripts/preload.js"
  }
}
