{
  "name": "@storybook/marko",
  "description": "Storybook for Marko: Develop Marko Components in isolation.",
  "version": "7.1.1",
  "author": "Dylan Piercey <dpiercey@ebay.com>",
  "bin": {
    "build-storybook": "bin/build.js",
    "start-storybook": "bin/index.js",
    "storybook-server": "bin/index.js"
  },
  "bugs": "https://github.com/storybookjs/marko/issues",
  "dependencies": {
    "@marko/webpack": "^9.0.3",
    "@storybook/core": "^6.3.0",
    "@storybook/core-common": "^6.3.0",
    "escalade": "^3.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@marko/compiler": "^5.10.7",
    "@marko/testing-library": "^5.0.4",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/builder-webpack5": "^6.3.0",
    "@storybook/manager-webpack5": "^6.3.0",
    "@types/babel__core": "^7.1.14",
    "@types/express": "^4.17.12",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.4",
    "@types/webpack": "^4.41.29",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "codecov": "^3.8.2",
    "coverage-istanbul-loader": "^3.0.5",
    "esbuild": "^0.12.9",
    "esbuild-register": "^2.6.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fixpack": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "marko": "^5.10.4",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "playwright": "^1.12.2",
    "prettier": "^2.3.1",
    "standard-version": "^9.3.0",
    "tiny-glob": "^0.2.9",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "bin",
    "dist",
    "testing",
    "standalone",
    "!**/__tests__",
    "!*.tsbuildinfo"
  ],
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  },
  "homepage": "https://github.com/storybookjs/marko",
  "jsnext": "dist/cjs/client/index.js",
  "keywords": [
    "marko",
    "playground",
    "preview",
    "storybook"
  ],
  "license": "MIT",
  "main": "dist/cjs/client/index.js",
  "module": "dist/esm/client/index.js",
  "peerDependencies": {
    "marko": "^5",
    "webpack": "^4 || ^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/marko"
  },
  "scripts": {
    "build": "node build.js && tsc",
    "ci:report": "codecov",
    "ci:test": "NODE_ENV=test nyc mocha -- --forbid-pending --forbid-only",
    "format": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write && (fixpack || true)",
    "lint": "tsc && npm run lint:eslint && npm run lint:prettier -- -l && fixpack",
    "lint:eslint": "eslint -f visualstudio .",
    "lint:prettier": "prettier '**/*{.ts,.js,.json,.md,.yml,rc}'",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "report": "open ./coverage/lcov-report/index.html",
    "storybook": "node --enable-source-maps -r esbuild-register ./src/server/index.ts --ci -p 8080 --loglevel warn -c ./src/__tests__/.storybook",
    "test": "NODE_ENV=test mocha --watch",
    "test:inspect": "PWDEBUG=1 npm run test -- --inspect"
  },
  "types": "dist/client/index.d.ts"
}
