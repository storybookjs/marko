{
  "name": "@storybook/marko",
  "description": "Storybook for Marko: Develop Marko Components in isolation.",
  "version": "7.2.0",
  "author": "Dylan Piercey <dpiercey@ebay.com>",
  "bin": {
    "build-storybook": "bin/build.js",
    "start-storybook": "bin/index.js",
    "storybook-server": "bin/index.js"
  },
  "bugs": "https://github.com/storybookjs/marko/issues",
  "dependencies": {
    "@marko/webpack": "^9.0.3",
    "@storybook/client-api": "^6.3.0",
    "@storybook/core": "^6.3.0",
    "@storybook/core-client": "^6.3.0",
    "@storybook/core-common": "^6.3.0",
    "escalade": "^3.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@marko/compiler": "^5.15.10",
    "@marko/testing-library": "^5.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/builder-webpack5": "^6.3.8",
    "@storybook/manager-webpack5": "^6.3.8",
    "@types/babel__core": "^7.1.16",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "codecov": "^3.8.3",
    "coverage-istanbul-loader": "^3.0.5",
    "esbuild": "^0.12.28",
    "esbuild-register": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "fast-glob": "^3.2.7",
    "fixpack": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "marko": "^5.10.4",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "playwright": "^1.14.1",
    "prettier": "^2.4.0",
    "semantic-release": "^17.4.7",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "bin",
    "dist",
    "testing",
    "standalone",
    "!**/__tests__",
    "!**/*.tsbuildinfo"
  ],
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  },
  "homepage": "https://github.com/storybookjs/marko",
  "jsnext": "dist/client/index.mjs",
  "keywords": [
    "marko",
    "playground",
    "preview",
    "storybook"
  ],
  "license": "MIT",
  "main": "dist/client/index.js",
  "module": "dist/client/index.mjs",
  "peerDependencies": {
    "marko": "^5",
    "webpack": "^4 || ^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/marko"
  },
  "scripts": {
    "build": "tsc -b && node -r esbuild-register build",
    "ci:report": "codecov",
    "ci:test": "nyc npm run mocha -- --forbid-pending --forbid-only",
    "format": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write && (fixpack || true)",
    "lint": "tsc -b && npm run lint:eslint && npm run lint:prettier -- -l && fixpack",
    "lint:eslint": "eslint -f visualstudio .",
    "lint:prettier": "prettier '**/*{.ts,.js,.json,.md,.yml,rc}'",
    "mocha": "NODE_ENV=development mocha 'src/**/__tests__/*/test.ts'",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "release": "semantic-release",
    "report": "open ./coverage/lcov-report/index.html",
    "storybook": "node --enable-source-maps -r esbuild-register ./src/server/index.ts --ci -p 8080 --loglevel warn -c ./src/__tests__/.storybook",
    "test": "npm run mocha -- --watch",
    "test:inspect": "PWDEBUG=1 npm test -- --inspect"
  },
  "types": "dist/client/index.d.ts"
}
